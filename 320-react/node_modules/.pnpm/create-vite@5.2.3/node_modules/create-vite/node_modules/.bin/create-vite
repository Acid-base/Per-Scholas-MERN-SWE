#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Per-Scholas-MERN-SWE/320-react/node_modules/.pnpm/create-vite@5.2.3/node_modules/create-vite/node_modules:/mnt/d/Per-Scholas-MERN-SWE/320-react/node_modules/.pnpm/create-vite@5.2.3/node_modules:/mnt/d/Per-Scholas-MERN-SWE/320-react/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Per-Scholas-MERN-SWE/320-react/node_modules/.pnpm/create-vite@5.2.3/node_modules/create-vite/node_modules:/mnt/d/Per-Scholas-MERN-SWE/320-react/node_modules/.pnpm/create-vite@5.2.3/node_modules:/mnt/d/Per-Scholas-MERN-SWE/320-react/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../index.js" "$@"
else
  exec node  "$basedir/../../index.js" "$@"
fi
